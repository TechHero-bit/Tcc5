
@{
    ViewBag.Title = "CadastroLista";
}

<h2>CadastroLista</h2>

<table class="table table-bordered table-hover table-responsive">
    <tr>
        <th>Id</th>
        <th>Id_tipofunc</th>
        <th>Id_seção</th>
        <th>Cpf</th>
        <th>Nome</th>
        <th>Senha</th>
        <th>Setor</th>
    </tr>

    @foreach (var item in ViewBag.Login)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Id_tipofunc</td>
            <td>@item.Id_secao</td>
            <td>@item.Cpf</td>
            <td>@item.Nome</td>
            <td>@item.Senha</td>
            <th>@item.Setor</th>
        </tr>

    }
</table>