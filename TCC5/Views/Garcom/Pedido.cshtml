
@{
    ViewBag.Title = "Pedido";
}

<h2>Pedido</h2>

<table class="table table-bordered table-hover table-responsive">
    <tr>
        <th>Id</th>
        <th>Comanda Id</th>
        <th>Data</th>
       
    </tr>

    @foreach (var item in ViewBag.Pedido)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Comanda_id</td>
            <td>@item.Data</td>
        </tr>

    }
</table>